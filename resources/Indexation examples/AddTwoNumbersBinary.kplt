type Term {
    max {
		a_0_$p$ -> a_0_$p$ . :0<=p<=3
		a_1_$p$ -> a_1_$p$ . :0<=p<=3
    }
}

type Add {
	2a_0_$p$ -> 2a_0_$p+1$ . :0<=p<=3
    a_0_$p$ + a_0_$p$ -> a_0_$p$ .  :0<=p<=3
    a_0_$p$ + a_1_$p$ -> a_1_$p$ .  :0<=p<=3
    a_1_$p$ + a_0_$p$ -> a_1_$p$ .  :0<=p<=3
    a_1_$p$ + a_1_$p$ -> a_0_$p+1$ . :0<=p<=3
}


termA {a_1_0, a_0_1, a_1_2} (Term) .  
termB {a_1_0, a_1_1, a_0_2} (Term) .  

add {} (Add) .

termA - add .
termB - add .
